<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans

http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="YandexSearchPage" class="ru.spring.temp3.pages.YandexSearchPage">
    </bean>

    <bean id="WebDriver" class="org.openqa.selenium.WebDriver"></bean>

    <bean id="DriverManager" class="ru.spring.temp3.helpers.DriverManager">
        <property name="WebDriver" ref="WebDriver"/>
    </bean>

    <bean class="ru.spring.temp3.pages.AbstractPage">
    </bean>

    <context:annotation-config/>
    <context:component-scan base-package="ru.spring.temp3"/>


</beans>